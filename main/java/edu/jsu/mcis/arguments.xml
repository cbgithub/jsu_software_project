<?xml version='1.0' encoding='us-ascii'?>
<program>
  <name>VolumeCalculator</name>
  <description>Calculate the volume of a box.</description>
    <arguments>
        <positional>
            <name>length</name>
            <type>float</type>
            <description>Calculate the volume of a box.</description>
            <position>1</position>
        </positional>
        <positional>
            <name>width</name>
            <type>float</type>
            <description>Calculate the volume of a box.</description>
            <position>2</position>
        </positional>
        <positional>
            <name>height</name>
            <type>float</type>
            <description>Calculate the volume of a box.</description>
            <position>3</position>
        </positional>
        <named>
            <name>type</name>
            <shortname>t</shortname>
            <type>string</type>
            <default>box</default>
        </named>
        <named>
            <name>digits</name>
            <shortname>d</shortname>
            <type>integer</type>
            <default>4</default>
        </named>
    </arguments>
</program>
